[Unit]
Description=port forward
After=sysinit.target

[Service]
ExecStart=/usr/bin/ssh root@13.78.50.153 -NTC -i /home/pi/.ssh/id_rsa -L 1883:13.78.50.153:1883 -o ServerAliveInterval=60 -o ExitOnForwardFailure=yes -o StrictHostKeyChecking=no
Restart=always
RestartSec=3

[Install]
WantedBy = multi-user.target
