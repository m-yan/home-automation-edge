[Unit]
Description=Port Forward
After=sysinit.target

[Service]
ExecStart=/usr/bin/ssh root@52.246.186.209 -NTC -i /home/pi/.ssh/id_rsa -L 1883:localhost:1883 -o ServerAliveInterval=60 -o ExitOnForwardFailure=yes -o StrictHostKeyChecking=no
Restart=always
RestartSec=3

[Install]
WantedBy = multi-user.target
